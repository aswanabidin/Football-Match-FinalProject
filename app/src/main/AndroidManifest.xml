<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="aswanabidin.football_match_finalproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:name=".FootballApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".features.events.detail.EventsDetailActivity">
        </activity>
        <activity android:name=".features.events.search.EventsSearchActivity">
        </activity>
        <activity android:name=".features.players.detail.PlayersDetailActivity">
        </activity>
        <activity android:name=".features.teams.detail.TeamsDetailActivity">
        </activity>
        <activity android:name=".features.teams.search.TeamsSearchActivity">
        </activity>
    </application>

</manifest>